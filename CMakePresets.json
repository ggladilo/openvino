{
    "version": 3,
    "configurePresets": [
        {
            "name": "BuildOptimization",
            "hidden": true,
            "generator": "Ninja",
            "cacheVariables": {
                "CMAKE_C_COMPILER_LAUNCHER": {
                    "type": "FILEPATH",
                    "value": "/usr/bin/ccache"
                },
                "CMAKE_CXX_COMPILER_LAUNCHER": {
                    "type": "FILEPATH",
                    "value": "/usr/bin/ccache"
                },
                "ENABLE_FASTER_BUILD": true
            }
        },
        {
            "name": "LinkerOptimization",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_EXE_LINKER_FLAGS": {
                    "type": "STRING",
                    "value": "-fuse-ld=gold -Wl,--threads -Wl,--thread-count,32"
                },
                "CMAKE_MODULE_LINKER_FLAGS": {
                    "type": "STRING",
                    "value": "-fuse-ld=gold -Wl,--threads -Wl,--thread-count,32"
                },
                "CMAKE_SHARED_LINKER_FLAGS": {
                    "type": "STRING",
                    "value": "-fuse-ld=gold -Wl,--threads -Wl,--thread-count,32"
                }
            }
        },
        {
            "name": "EnableTests",
            "hidden": true,
            "cacheVariables": {
                "ENABLE_TESTS": true,
                "ENABLE_FUNCTIONAL_TESTS": true,
                "ENABLE_BEH_TESTS": true
            }
        },
        {
            "name": "EnableModelsAndValidationSet",
            "hidden": true,
            "cacheVariables": {
                "ENABLE_MODELS": true,
                "ENABLE_VALIDATION_SET": true
            }
        },
        {
            "name": "Disabler",
            "hidden": true,
            "description": "Disables all options. It is used as a base for configuration where is only needed option are enabled to speed-up build. Should be used as the first argument to inherits.",
            "cacheVariables": {
                "ENABLE_TESTS": false,
                "ENABLE_FUNCTIONAL_TESTS": false,
                "ENABLE_BEH_TESTS": false,
                "ENABLE_MYRIAD_MVNC_TESTS": false,
                "ENABLE_GAPI_TESTS": false,
                "ENABLE_OV_CORE_UNIT_TESTS": false,
                "ENABLE_OV_CORE_BACKEND_UNIT_TESTS": false,
                "ENABLE_SAMPLES": false,
                "ENABLE_DATA": false,
                "ENABLE_OPENCV": false,
                "ENABLE_MYRIAD_NO_BOOT": false,
                "ENABLE_INTEL_CPU": false,
                "ENABLE_CLDNN": false,
                "ENABLE_INTEL_GPU": false,
                "ENABLE_ONEDNN_FOR_GPU": false,
                "ENABLE_INTEL_GNA": false,
                "ENABLE_V7_SERIALIZE": false,
                "ENABLE_OPENVINO_DEBUG": false,
                "ENABLE_DEBUG_CAPS": false,
                "ENABLE_GPU_DEBUG_CAPS": false,
                "ENABLE_CPU_DEBUG_CAPS": false,
                "ENABLE_LTO": false,
                "ENABLE_FASTER_BUILD": false,
                "ENABLE_CPPLINT": false,
